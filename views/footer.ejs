						function showResultsSource(eltId, val, listId, divId, hiddenFieldId) {
							///////////////////////////
							//alert('show result debounce');
							//return;
							////////////////////
							res = document.getElementById(`${divId}`);
							res.innerHTML = '';

							if (val == '') {
								return;
							}
							let list = '';
							fetch('/Moqflight/getAirportSuggestion?keyword=' + val).then(function (response) {
								return response.json();
							}).then(function (data) {
								//res.innerHTML = `<div class = "centered"><div class = "blob-1"></div><div class = "blob-2"></div></div>`;
								for (i = 0; i < data.length; i++) {
									if (typeof data[i].detailedName != 'undefined')
										list += '<tr> <td style="cursor: pointer; color: #3D71E9; font-weight: 600px; background-color: #f7f7f; border-width: 2px;"' + 'data-value=' + data[i].iataCode + '>' + data[i].detailedName + "--" +
											data[i].subType + '</td> </tr>';
								}
								//res.innerHTML = `<ul id=sourceFlightList>` + list + '</ul>';
								res.innerHTML = `<table class="table table-hover" style="width:100%; border-width: 3px;" id=${listId} >` + list + '</table>';
