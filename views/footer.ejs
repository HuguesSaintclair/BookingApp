								$(`table[id*=${listId}] tr td`).click(function () {
									// document.getElementById("sourceFlightCode").value = "";
									document.getElementById(`${hiddenFieldId}`).value = "";
									document.getElementById(`${hiddenFieldId}`).value = $(this).data('value');
									//alert($(this).attr('data-value').val());
									document.getElementById(`${eltId}`).value = "";
									document.getElementById(`${eltId}`).value = $(this).text();
									$(`#${divId}`).empty();
									return;
								});
								$('.alert').show();
								return true;
							}).catch(function (err) {
